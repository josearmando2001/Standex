Options All -Indexes
RewriteEngine on
RewriteCond %{SCRIPT_FILENAME} !-d
RewriteCond %{SCRIPT_FILENAME} !-f
# Ruta home de acceso para el index.php
RewriteRule ^home/?$ index.php
# Ruta consulta-clientes de acceso para el archivo templates/consulta.php
RewriteRule ^consulta-clientes/?$ templates/consulta.php